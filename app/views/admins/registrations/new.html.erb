<article>
<h2>Add Administrator</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

	<dl class="inline">
		<dt><%= f.label :username %></dt>
		<dd><%= f.text_field :username, :tabindex => 1 %></dd>

		<dt><%= f.label :email %></dt>
		<dd><%= f.text_field :email, :tabindex => 2 %></dd>


		<dt><%= f.label :password %></dt>
		<dd><%= f.password_field :password, :tabindex => 3 %></dd>

		<dt><%= f.label :password_confirmation %></dt>
		<dd><%= f.password_field :password_confirmation, :tabindex => 4 %>
			<%= f.submit "Save", :class => "button", :tabindex => 5 %>
		</dd>

<% end %>

<%= render :partial => "devise/shared/links" %>


</article>