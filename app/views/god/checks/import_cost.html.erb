<article>
	<h1>Re-import Cost</h1>
	
	<%= render :partial => 'shared/errors', :locals => {:object => @check} %>
	
	<%= form_for @check, :url => do_import_cost_god_checks_path, :html => {:class => 'uniform', :multipart => true} do |f| %>
		<fieldset>
		<legend>Item excel</legend>
		<dl class='inline'>

			<dt><%= f.label :description, 'Inventory Name' %></dt>
			<dd><%= @check.description %></dd>

			<dt><%= f.label :reimport_cost_xls, "Items.xls" %></dt>
			<dd>
				<%= f.file_field :reimport_cost_xls %>
				<small>Import latest AO Cost into SCS</small>
				<small>ALL ORDER: Reports -> Items -> Items</small>
			</dd>

		</dl>
		</fieldset>

		<div class="buttons">
			<%= loading_submit f, "Re import", :class => 'button' %>
		</div>
	<% end %>
</article>

