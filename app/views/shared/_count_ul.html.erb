<ul>
	<% if curr_check && curr_check.state == 'open' %>
	<li <%= "class='current'" if @sub_menu == :entry && @c_i == count %>><%= link_to 'Data Entry', counts_path(:count => count) %></li>
		<% if count <= 2 %>
		<li <%= "class='current'" if @sub_menu == :missing && @c_i == count %>><%= link_to "Missing Tags (#{Tag.in_check(curr_check.id).not_finish(count).count})", missing_tag_counts_path(:count => count) %></li>
		<% end %>
	<% end %>
	<li <%= "class='current'" if @sub_menu == :result && @c_i == count %>><%= link_to "Count #{count} Result", result_counts_path(:count => count) %></li>
	<% if count <= 2 && curr_check.finish_count_in(count) %>
	<li <%= "class='current'" if @sub_menu == :report && @c_i == count %>><%= link_to "Count #{count} Result VS Frozen", frozen_report_counts_path(:count => count) %></li>
	<% end %>
</ul>