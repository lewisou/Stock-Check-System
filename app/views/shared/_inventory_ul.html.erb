<ul>
	<% if curr_check && curr_check.state == 'open' %>
	<li <%= "class='current'" if @sub_menu == :missing_cost %>><%= link_to "Missing Cost (#{curr_check.items.missing_cost.count})", missing_cost_adm_items_path %></li>
	<% end %>
	<li>
		<%= link_to "Onsite Tickets", adm_tags_path %>
		<ul>
		<li <%= "class='current'" if @sub_menu == :tags %>><%= link_to "Tickets (Edit / Delete)", adm_tags_path %></li>
		<% if allow_data_entry? %>
		<li <%= "class='current'" if @sub_menu == :items %>><%= link_to "Create Onsite Ticket", adm_items_path(:sub_menu => :items, :type => :onsite) %></li>
		<% end %>
		<li <%= "class='current'" if @sub_menu == :print_list %>><%= link_to "Print list (#{Tag.in_check(curr_check.id).ready_to_print.count} to print)", adm_wait_prints_path %></li>
		<li <%= "class='current'" if @sub_menu == :missing_count_1 %>><%= link_to "Count 1 Missing Tickets (#{Tag.in_check(curr_check.id).not_finish(1).countable.count})", missing_tag_adm_counts_path(:count => 1) %></li>
		<li <%= "class='current'" if @sub_menu == :result_count_1 %>><%= link_to "Count 1 Result (#{Tag.in_check(curr_check.id).finish(1).countable.count})", result_adm_counts_path(:count => 1) %></li>
		<li <%= "class='current'" if @sub_menu == :missing_count_2 %>><%= link_to "Count 2 Missing Tickets (#{Tag.in_check(curr_check.id).not_finish(2).countable.count})", missing_tag_adm_counts_path(:count => 2) %></li>
		<li <%= "class='current'" if @sub_menu == :result_count_2 %>><%= link_to "Count 2 Result (#{Tag.in_check(curr_check.id).finish(2).countable.count})", result_adm_counts_path(:count => 2) %></li>
		<li <%= "class='current'" if @sub_menu == :result_count_3 %>><%= link_to "Count 3 Result (#{Tag.in_check(curr_check.id).finish(3).countable.count})", result_adm_counts_path(:count => 3) %></li>
		<li <%= "class='current'" if @sub_menu == :final_qty %>><%= link_to "Final adjustment input", adj_list_adm_tags_path %></li>
		</ul>
	</li>
	<li <%= "class='current'" if @sub_menu == :remote %>><%= link_to "Remote Warehouses Tickets (#{curr_check.inventories.remote_s.where(:inputed_qty => nil).count})", adm_inventories_path %></li>
	<li <%= "class='current'" if @sub_menu == :counters %>><%= link_to "Participants (#{Counter.count})", adm_counters_path %></li>
	<li>
		<a href='#'>Reports</a>
		<ul>
			<li <%= "class='current'" if @sub_menu == :variance %>><%= link_to 'Variance Count 1 VS 2', count_varience_check_reports_path(curr_check) %></li>
			<li <%= "class='current'" if @sub_menu == :count_1_frozen %>><%= link_to 'Count Result VS Frozen QTY', count_frozen_check_reports_path(curr_check) %></li>
			<li <%= "class='current'" if @sub_menu == :report_tags %>><%= link_to 'Summary of counts by location', tags_check_reports_path(curr_check) %></li>
			<li <%= "class='current'" if @sub_menu == :report_inventories %>><%= link_to 'Summary by counts by value', inventories_check_reports_path(curr_check) %></li>

		</ul>
	</li>
</ul>
